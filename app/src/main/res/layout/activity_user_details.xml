<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/UserDetailspage"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".UserDetails"
    android:background="@color/black">

    <TextView
        android:id="@+id/UserDetailsView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="User Details"
        android:textColor="#ffff"
        android:textSize="24sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.068"/>

    <TextView
        android:id="@+id/NameView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Name:"
        android:textColor="#ffff"
        android:textSize="22sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.086"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/UserDetailsView"
        app:layout_constraintVertical_bias="0.049"/>

    <TextView
        android:id="@+id/DisplayName"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Here display the name fron DB"
        android:textColor="#ffff"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.71"
        app:layout_constraintStart_toEndOf="@+id/NameView"
        app:layout_constraintTop_toBottomOf="@+id/UserDetailsView"
        app:layout_constraintVertical_bias="0.065"/>

    <TextView
        android:id="@+id/PhoneNumberView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Phone:"
        android:textColor="#ffff"
        android:textSize="22sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.087"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/NameView"
        app:layout_constraintVertical_bias="0.053"/>

    <TextView
        android:id="@+id/DisplayPhoneNumber"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textColor="#ffff"
        android:text="Here display the phone from DB"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.773"
        app:layout_constraintStart_toEndOf="@+id/PhoneNumberView"
        app:layout_constraintTop_toBottomOf="@+id/DisplayName"
        app:layout_constraintVertical_bias="0.068"/>

    <TextView
        android:id="@+id/GovtIDView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Govt ID No:"
        android:textColor="#ffff"
        android:textSize="22sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.093"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/PhoneNumberView"
        app:layout_constraintVertical_bias="0.064"/>

    <TextView
        android:id="@+id/DisplayGovtID"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textColor="#ffff"
        android:text="Here display the govtID from DB"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.647"
        app:layout_constraintStart_toEndOf="@+id/GovtIDView"
        app:layout_constraintTop_toBottomOf="@+id/DisplayPhoneNumber"
        app:layout_constraintVertical_bias="0.088"/>

    <TextView
        android:id="@+id/FirstDoseView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="First Dose:"
        android:textColor="#ffff"
        android:textSize="22dp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.09"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/GovtIDView"
        app:layout_constraintVertical_bias="0.058"/>

    <TextView
        android:id="@+id/DisplayFirstDose"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Display the date of first dose from DB"
        android:textColor="#ffff"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.75"
        app:layout_constraintStart_toEndOf="@+id/FirstDoseView"
        app:layout_constraintTop_toBottomOf="@+id/DisplayGovtID"
        app:layout_constraintVertical_bias="0.071"/>

    <TextView
        android:id="@+id/SecondDoseView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Second Dose:"
        android:textColor="#ffff"
        android:textSize="22dp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.101"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/FirstDoseView"
        app:layout_constraintVertical_bias="0.081" />

    <TextView
        android:id="@+id/DisplaySecondDose"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="display the date of 2nd dose from DB"
        android:textColor="#ffff"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.333"
        app:layout_constraintStart_toEndOf="@+id/SecondDoseView"
        app:layout_constraintTop_toBottomOf="@+id/DisplayFirstDose"
        app:layout_constraintVertical_bias="0.103" />

    <TextView
        android:id="@+id/DoseQueryView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Have you had your first dose?"
        android:textColor="#fcff47"
        android:textSize="18sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.472"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/UserDetailsView"
        app:layout_constraintVertical_bias="0.742"/>

    <Button
        android:id="@+id/YesBtn"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Yes"
        app:backgroundTint="#6B9A33"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/NoBtn"
        app:layout_constraintHorizontal_bias="0.63"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/DoseQueryView"
        app:layout_constraintVertical_bias="0.363"/>

    <Button
        android:id="@+id/NoBtn"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="No"
        app:backgroundTint="#EA1212"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.715"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/DoseQueryView"
        app:layout_constraintVertical_bias="0.357"/>

    <TextView
        android:id="@+id/VaccineTypeView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Vaccine Type:"
        android:textColor="#ffff"
        android:textSize="21sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.107"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/SecondDoseView"
        app:layout_constraintVertical_bias="0.095"/>

    <TextView
        android:id="@+id/DisplayVaccineType"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Display Vccine type from DB"
        android:textColor="#ffff"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.795"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/DisplaySecondDose"
        app:layout_constraintVertical_bias="0.124" />
</androidx.constraintlayout.widget.ConstraintLayout>